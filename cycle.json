{
  "version": "1.0",
  "about": {
    "version": "1.0",
    "description": "a base image for nginx and apache"
  },
  "test": [],
  "services": {
    "loadbalancer": null,
    "vpn": null
  },
  "containers": {
    "nginx": {
      "name": "nginx",
      "image": {
        "name": "nginx:alpine",
        "source": {
          "repo": {
            "dir": "/",
            "build_file": "/nginx/Dockerfile",
            "url": "https://github.com/aubuchcl/docker_nginx_httpd.git"
          }
        }
      },
      "config": {
        "network": {
          "public": "disable",
          "hostname": "testing",
          "ports": ["6379:6379"]
        }
  }
}
}
}