{
  "version": "1.0",
  "containers": {
    "demo": {
      "name": "demo",
      "image": {
        "name": "simplews",
        "origin": {
          "type": "docker-file",
          "details": {
            "dir": "/",
            "path": "/Dockerfile"
          }
        }
      },
      "config": {
        "network": {
          "public": "enable",
          "hostname": "simplews",
          "ports": ["80:80"]
        },
        "deploy": {
          "instances": 1
        }
      }
    },
    "pub": {
      "name": "pub",
      "image": {
        "name": "pub",
        "origin": {
          "type": "docker-hub",
          "details": {
            "target": "redis:latest"
          }
        }
      },
      "config": {
        "network": {
          "public": "enable",
          "hostname": "pub",
          "ports": ["80:6379"]
        },
        "deploy": {
          "instances": 1
        }
      }
    }
  }
}